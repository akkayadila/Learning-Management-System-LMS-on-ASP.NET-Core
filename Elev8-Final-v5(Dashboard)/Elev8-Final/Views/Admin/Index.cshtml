@{
    Layout = "_LayoutDashboard";
}

<h1>Admin Panel</h1>

<div class="row">
    <div class="col-md-12">
        <form method="get">
            <div class="input-group">
                <input type="text" class="form-control" placeholder="Search by course, user, or assignment name" name="search">
                <div class="input-group-append">
                    <button class="btn btn-primary" type="submit">
                        <i class="fa fa-search"></i>
                    </button>
                </div>
            </div>
        </form>
    </div>
</div>
